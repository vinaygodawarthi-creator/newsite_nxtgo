        /* Modern Tracking Modal Styles */
        .modal-2 {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            z-index: 9999;
            display: flex !important;
            align-items: center;
            justify-content: center;
            opacity: 0;
            transition: opacity 0.4s ease;
            pointer-events: none;
            background: rgba(0, 0, 0, 0);
        }

        .modal-2.active {
            opacity: 1;
            pointer-events: auto;
        }

        .modalbg {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.6);
            backdrop-filter: blur(2px);
            z-index: 1;
        }

        .modal-container-2 {
            position: relative;
            background: linear-gradient(135deg, #f5f7fa 0%, #ffffff 100%);
            border-radius: 24px;
            padding: 3rem 2.5rem;
            max-width: 500px;
            width: 90%;
            z-index: 2;
            box-shadow: 0 20px 60px rgba(0, 0, 0, 0.3), 0 0 1px rgba(0, 0, 0, 0.1);
            max-height: 90vh;
            overflow-y: auto;
            animation: modalSlideIn 0.5s cubic-bezier(0.34, 1.56, 0.64, 1);
        }

        @keyframes modalSlideIn {
            from {
                opacity: 0;
                transform: scale(0.9) translateY(30px);
            }
            to {
                opacity: 1;
                transform: scale(1) translateY(0);
            }
        }

        .link-block-12 {
            position: absolute;
            top: 1.5rem;
            right: 1.5rem;
            width: 40px;
            height: 40px;
            cursor: pointer;
            background: rgba(0, 82, 204, 0.1);
            border: none;
            border-radius: 50%;
            padding: 0;
            display: flex;
            align-items: center;
            justify-content: center;
            transition: all 0.3s ease;
            line-height: 1;
        }

        .link-block-12:hover {
            background: rgba(0, 82, 204, 0.2);
            transform: rotate(90deg);
        }

        .link-block-12:active {
            transform: rotate(90deg) scale(0.95);
        }

        .close-text {
            font-size: 1.8rem;
            font-weight: bold;
            cursor: pointer;
            color: #0052cc;
            transition: color 0.2s;
        }

        .link-block-12:hover .close-text {
            color: #0038a6;
        }

        /* Modal Layout - Two Column */
        .modal-content-wrapper {
            display: grid;
            grid-template-columns: 1fr 1.5fr;
            gap: 3rem;
            align-items: center;
        }

        /* Left Side - Logo */
        .modal-left-side {
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
        }

        .div-block-58 {
            text-align: center;
            margin-bottom: 1rem;
            display: flex;
            align-items: center;
            justify-content: center;
        }

        .image-5 {
            height: 60px;
            max-width: 180px;
            object-fit: contain;
            filter: drop-shadow(0 4px 6px rgba(0, 0, 0, 0.1));
        }

        .logo-text {
            font-size: 0.85rem;
            color: #666;
            margin-top: 1rem;
            font-weight: 500;
            letter-spacing: 0.05em;
        }

        /* Right Side - Form */
        .modal-right-side {
            display: flex;
            flex-direction: column;
            justify-content: center;
        }

        .div-block-57 {
            text-align: left;
        }

        .form-heading {
            font-size: 2rem;
            font-weight: 800;
            margin: 0 0 0.5rem 0;
            color: #1a1a1a;
            letter-spacing: -0.5px;
        }

        .form-subheading {
            font-size: 0.95rem;
            color: #888;
            margin-bottom: 1.5rem;
            font-weight: 400;
        }

        .form-block {
            width: 100%;
        }

        .form {
            display: flex;
            flex-direction: column;
            gap: 1.2rem;
        }

        .form-group {
            display: flex;
            flex-direction: column;
            gap: 0.6rem;
        }

        .form-label {
            font-size: 0.9rem;
            font-weight: 600;
            color: #333;
            letter-spacing: 0.02em;
        }

        .input-form {
            padding: 0.95rem 1.2rem;
            border: 2px solid #e0e0e0;
            border-radius: 10px;
            font-size: 1rem;
            width: 100%;
            box-sizing: border-box;
            transition: all 0.3s ease;
            background: #f9f9f9;
            font-family: 'Roboto', sans-serif;
            letter-spacing: 0.1em;
        }

        .input-form::placeholder {
            color: #bbb;
            letter-spacing: 0.05em;
        }

        .input-form:focus {
            outline: none;
            border-color: #0052cc;
            background: white;
            box-shadow: 0 0 0 4px rgba(0, 82, 204, 0.1), inset 0 0 0 1px rgba(0, 82, 204, 0.05);
            transform: translateY(-2px);
        }

        .input-form:hover:not(:focus) {
            border-color: #ccc;
            background: #fafafa;
        }

        /* Submit Button */
        .button-submit {
            padding: 1rem 1.5rem;
            background: linear-gradient(135deg, #0052cc 0%, #0038a6 100%);
            color: white;
            border: none;
            border-radius: 10px;
            font-size: 1rem;
            font-weight: 600;
            cursor: pointer;
            transition: all 0.3s ease;
            margin-top: 0.5rem;
            text-transform: uppercase;
            letter-spacing: 0.1em;
            box-shadow: 0 4px 15px rgba(0, 82, 204, 0.3);
        }

        .button-submit:hover {
            transform: translateY(-2px);
            box-shadow: 0 6px 20px rgba(0, 82, 204, 0.4);
        }

        .button-submit:active {
            transform: translateY(0);
            box-shadow: 0 2px 10px rgba(0, 82, 204, 0.3);
        }

        .button-submit:disabled {
            background: #ccc;
            cursor: not-allowed;
            box-shadow: none;
            transform: none;
        }

        /* Loading and Result States */
        #tracking-result {
            color: #d32f2f;
            font-size: 0.9rem;
            margin-top: 1rem;
            padding: 1rem;
            background: rgba(211, 47, 47, 0.08);
            border-radius: 8px;
            border-left: 3px solid #d32f2f;
            display: none;
        }

        #tracking-result.success {
            color: #2e7d32;
            background: rgba(46, 125, 50, 0.08);
            border-left-color: #2e7d32;
        }

        #tracking-result.loading {
            color: #0052cc;
            background: rgba(0, 82, 204, 0.08);
            border-left-color: #0052cc;
        }

        .loading-spinner {
            display: inline-block;
            width: 14px;
            height: 14px;
            margin-right: 8px;
            border: 2px solid rgba(0, 82, 204, 0.3);
            border-top-color: #0052cc;
            border-radius: 50%;
            animation: spin 0.8s linear infinite;
            vertical-align: middle;
        }

        @keyframes spin {
            to { transform: rotate(360deg); }
        }

        /* Decorative Elements */
        .modal-decoration {
            position: absolute;
            width: 300px;
            height: 300px;
            background: radial-gradient(circle, rgba(0, 82, 204, 0.08) 0%, transparent 70%);
            border-radius: 50%;
            pointer-events: none;
        }

        .decoration-1 {
            top: -100px;
            right: -100px;
        }

        .decoration-2 {
            bottom: -80px;
            left: -80px;
        }

        /* Responsive Design */
        @media (max-width: 900px) {
            .modal-container-2 {
                padding: 2rem 1.5rem;
            }

            .modal-content-wrapper {
                grid-template-columns: 1fr;
                gap: 2rem;
            }

            .modal-left-side {
                display: none;
            }

            .form-heading {
                font-size: 1.75rem;
            }
        }

        @media (max-width: 768px) {
            .modal-2 {
                align-items: flex-end;
            }

            .modal-container-2 {
                padding: 2rem 1.5rem;
                width: 95%;
                max-width: 100%;
                border-radius: 24px 24px 0 0;
                animation: modalSlideUp 0.4s cubic-bezier(0.34, 1.56, 0.64, 1);
            }

            @keyframes modalSlideUp {
                from {
                    opacity: 0;
                    transform: translateY(100px);
                }
                to {
                    opacity: 1;
                    transform: translateY(0);
                }
            }

            .link-block-12 {
                top: 1rem;
                right: 1rem;
            }

            .form-heading {
                font-size: 1.5rem;
            }

            .form-subheading {
                font-size: 0.9rem;
            }

            .input-form {
                padding: 0.85rem 1rem;
                font-size: 16px;
            }

            .button-submit {
                padding: 0.9rem 1.2rem;
                font-size: 0.95rem;
            }

            .modal-decoration {
                display: none;
            }
        }

        @media (max-width: 600px) {
            .modal-container-2 {
                padding: 1.5rem 1.25rem;
            }

            .form-heading {
                font-size: 1.35rem;
            }

            .form-label {
                font-size: 0.85rem;
            }

            .input-form {
                padding: 0.8rem 1rem;
                font-size: 16px;
                border-radius: 8px;
            }

            .button-submit {
                padding: 0.85rem 1rem;
                font-size: 0.9rem;
            }

            #tracking-result {
                font-size: 0.85rem;
                padding: 0.8rem;
            }
        }

        @media (max-width: 400px) {
            .modal-container-2 {
                padding: 1.25rem 1rem;
            }

            .form-heading {
                font-size: 1.2rem;
            }

            .form-subheading {
                font-size: 0.85rem;
            }

            .link-block-12 {
                width: 36px;
                height: 36px;
            }

            .close-text {
                font-size: 1.5rem;
            }
        }
