    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=64adecd85321f8b8806eb119" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="https://assets-global.website-files.com/64adecd85321f8b8806eb119/js/webflow.b2db09131.js" type="text/javascript"></script>
    
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Modal elements
            const modal = document.getElementById('trackingModal');
            const trackShipmentBtn = document.getElementById('trackShipmentBtn');
            const closeModalBtn = document.getElementById('closeModalBtn');
            const modalOverlay = document.getElementById('modalOverlay');
            const trackingForm = document.getElementById('tracking-form');
            const trackingInput = document.getElementById('tracking_number');
            const trackingResult = document.getElementById('tracking-result');
            const resultIcon = document.getElementById('result-icon');
            const resultText = document.getElementById('result-text');

            // Prevent form submission on Enter key (handled by submit button)
            trackingInput?.addEventListener('keypress', function(e) {
                if (e.key === 'Enter') {
                    e.preventDefault();
                    trackingForm?.dispatchEvent(new Event('submit'));
                }
            });

            // Format input to only accept numbers
            trackingInput?.addEventListener('input', function(e) {
                this.value = this.value.replace(/[^0-9]/g, '').slice(0, 8);
            });

            // Open modal on button click
            if (trackShipmentBtn) {
                trackShipmentBtn.addEventListener('click', function(e) {
                    e.preventDefault();
                    openModal();
                });
            }

            // Close modal on close button click
            if (closeModalBtn) {
                closeModalBtn.addEventListener('click', function(e) {
                    e.preventDefault();
                    closeModal();
                });
            }

            // Close modal on overlay click
            if (modalOverlay) {
                modalOverlay.addEventListener('click', function() {
                    closeModal();
                });
            }

            // Close modal on Escape key
            document.addEventListener('keydown', function(e) {
                if (e.key === 'Escape' && modal?.classList.contains('active')) {
                    closeModal();
                }
            });

            // Modal functions
            function openModal() {
                if (modal) {
                    modal.classList.add('active');
                    modal.style.display = 'flex';
                    document.body.style.overflow = 'hidden';
                    trackingInput?.focus();
                    trackingInput?.select?.();
                }
            }

            function closeModal() {
                if (modal) {
                    modal.classList.remove('active');
                    setTimeout(function() {
                        modal.style.display = 'none';
                        document.body.style.overflow = 'auto';
                    }, 300);
                    trackingResult.style.display = 'none';
                    trackingInput.value = '';
                }
            }

            // Tracking form submission with enhanced functionality
            if (trackingForm) {
                trackingForm.addEventListener('submit', function(e) {
                    e.preventDefault();
                    
                    const trackingNumber = trackingInput.value.trim();
                    const submitBtn = trackingForm.querySelector('.button-submit');
                    
                    // Validate tracking number
                    if (!trackingNumber.match(/^[0-9]{8}$/)) {
                        showError('Invalid tracking number. Please enter exactly 8 digits.');
                        trackingInput.focus();
                        return;
                    }

                    // Disable submit button
                    if (submitBtn) {
                        submitBtn.disabled = true;
                        submitBtn.textContent = 'Searching...';
                    }

                    // Show loading state with spinner
                    trackingResult.classList.remove('success');
                    trackingResult.classList.add('loading');
                    resultIcon.innerHTML = '<span class="loading-spinner"></span>';
                    resultText.textContent = 'Searching for your shipment...';
                    trackingResult.style.display = 'block';

                    // Simulate API call (replace with actual API endpoint)
                    setTimeout(function() {
                        // Example: Check if tracking number is valid
                        const isValid = Math.random() > 0.5; // 50% chance for demo
                        
                        if (isValid) {
                            showSuccess(`Shipment ${trackingNumber} found! Your package is on the way.`);
                        } else {
                            showError(`No tracking data found for ${trackingNumber}.`);
                        }

                        // Re-enable submit button
                        if (submitBtn) {
                            submitBtn.disabled = false;
                            submitBtn.textContent = 'Submit';
                        }
                    }, 1500);
                });
            }

            // Success message handler
            function showSuccess(message) {
                trackingResult.classList.add('success');
                trackingResult.classList.remove('loading');
                resultIcon.innerHTML = ' ';
                resultText.textContent = message;
                trackingResult.style.display = 'block';
                
                // Auto-close after 3 seconds
                setTimeout(() => {
                    closeModal();
                }, 3000);
            }

            // Error message handler
            function showError(message) {
                trackingResult.classList.remove('success', 'loading');
                resultIcon.innerHTML = ' ';
                resultText.textContent = message;
                trackingResult.style.display = 'block';
            }

            // Mobile menu hamburger toggle
            const hamburger = document.querySelector('.hamburger-menu-wrapper');
            const navMenu = document.querySelector('.header-menu-wrapper');
            
            if (hamburger && navMenu) {
                hamburger.addEventListener('click', function() {
                    navMenu.classList.toggle('active');
                });

                // Close menu when a link is clicked
                const navLinks = navMenu.querySelectorAll('.nav-link');
                navLinks.forEach(function(link) {
                    link.addEventListener('click', function() {
                        navMenu.classList.remove('active');
                    });
                });
            }

            // Close menu on resize
            window.addEventListener('resize', function() {
                if (window.innerWidth > 767 && navMenu) {
                    navMenu.classList.remove('active');
                }
            });

            // Prevent scroll behind modal on mobile
            modal?.addEventListener('touchmove', function(e) {
                if (e.target === modalOverlay) {
                    e.preventDefault();
                }
            }, { passive: false });
        });
    </script>
